{
    "name": "faisalawan/shoppingcart",
    "description": "Laravel Shoppingcart",
    "keywords": ["laravel", "shoppingcart"],
    "license": "MIT",
    "authors": [
        {
            "name": "Faisal awan",
            "email": "faisalawan.live@gmail.com"
        }
    ],
    "require": {
        "illuminate/support": "@dev",
        "illuminate/session": "@dev",
        "illuminate/events": "@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "@dev",
        "mockery/mockery": "~0.9.0",
        "orchestra/testbench": "~3.1"
    },
    "autoload": {
        "psr-4": {
            "Gloudemans\\Shoppingcart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gloudemans\\Tests\\Shoppingcart\\": "tests/"
        }
    },
    "suggest": {
        "gloudemans/notify": "Simple flash notifications for Laravel"
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "Gloudemans\\Shoppingcart\\ShoppingcartServiceProvider"
            ],
            "aliases": {
                "Cart": "Gloudemans\\Shoppingcart\\Facades\\Cart"
            }
        }
    }
}
